<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_shop">

    <custom_fragment
        android:id="@+id/navigation_menu"
        android:name="com.example.mimiuchi.view.fragment.MenuFragment"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu"/>
    <custom_fragment
        android:id="@+id/navigation_coupon"
        android:name="com.example.mimiuchi.view.fragment.CouponFragment"
        android:label="fragment_coupon"
        tools:layout="@layout/fragment_coupon"/>
    <custom_fragment
        android:id="@+id/navigation_shop"
        android:name="com.example.mimiuchi.view.fragment.ShopFragment"
        android:label="fragment_shop"
        tools:layout="@layout/fragment_shop"/>
</navigation>